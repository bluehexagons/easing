{
  "name": "@bluehexagons/easing",
  "description": "A light library of easing functions",
  "version": "0.4.2",
  "license": "MIT",
  "author": "bluehexagons <this@bluehexagons.com>",
  "homepage": "https://github.com/bluehexagons/easing",

  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluehexagons/easing.git"
  },
  "bugs": {
    "url": "https://github.com/bluehexagons/easing/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/bluehexagons"
  },

  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=15.0.0"
  },
  "types": "./dist/main.d.ts",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./cjs/main.cjs",
      "default": "./dist/main.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },

  "scripts": {
    "cjs": "tsc --project tsconfig.cjs.json && node ./build.js",
    "test": "node test.mjs",
    "build": "tsc",
    "postbuild": "npm run cjs"
  },

  "keywords": [
    "easing",
    "interpolation",
    "lerp"
  ],
  "files": [
    "dist",
    "cjs"
  ],
  "devDependencies": {
    "typescript": "^5.4.2"
  }
}
